<Application x:Class="Filmweb.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Filmweb"
             xmlns:view="clr-namespace:Filmweb.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <local:NullToCollapsedGridLengthConverter x:Key="NullToCollapsedGridLengthConverter" />
        <local:NullToCollapsedVisibilityConverter x:Key="NullToCollapsedVisibilityConverter" />
    </Application.Resources>
</Application>
